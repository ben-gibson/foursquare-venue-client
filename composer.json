{
  "name": "ben-gibson/foursquare-venue-client",
  "type": "library",
  "description": "A PHP client library for accessing the Foursquare userless venue endpoint.",
  "keywords": [
    "ben-gibson",
    "foursquare-venue-client",
    "foursquare",
    "venue",
    "client",
    "api"
  ],
  "homepage": "https://github.com/ben-gibson/foursquare-venue-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Gibson",
      "email": "ben.gibson.2011@gmail.com",
      "homepage": "https://ben-gibson.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "beberlei/assert": "^2.6",
    "php-http/client-common": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/discovery": "^1.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "~5.0",
    "squizlabs/php_codesniffer": "~2.3",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/mock-client": "^0.3.2"
  },
  "autoload": {
    "psr-4": {
      "Gibbo\\Foursquare\\Client\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gibbo\\Foursquare\\ClientTests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
